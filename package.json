{
	"name": "wms-frontend",
	"version": "0.6.0",
	"type": "module",
	"description": "01星球大项目-10WMS项目。这里是WMS系统后台前端代码。",
	"packageManager": "pnpm@9.5.0",
	"engines": {
		"node": ">=20.15.0",
		"vscode": "^1.22.0",
		"pnpm": ">=9.5.0"
	},
	"scripts": {
		"dev": "turbo vite:dev",
		"build": "vite build --mode production",
		"deploy": "node --import=tsx ./scripts/deploy.ts",
		"preview": "vite preview",
		"test": "vitest --ui --watch --api.port=4000",
		"format": "prettier --write .",
		"docs:dev": "turbo do-docs-dev",
		"docs:build": "turbo do-docs-build",
		"git:fetch": "git fetch -p",
		"git:push": "git push --follow-tags",
		"git:commit": "git-cz",
		"vite:dev": "vite",
		"clear:deps": "rimraf -g **/{node_modules,pnpm-lock.yaml}",
		"clear:cache": "rimraf -g **/{dist,.turbo,.vercel,.cache,.temp}",
		"changeset:add": "changeset add",
		"changeset:version": "changeset version && changeset tag",
		"up-ruan-cat": "pnpm update '@ruan-cat/*' --latest",
		"lint": "eslint --fix .",
		"corepack:pnpm": "corepack enable && corepack prepare pnpm@9.5.0 --activate",
		"notuse-preinstall": "npx only-allow pnpm && npm run corepack:pnpm",
		"rm:types": "node --import=tsx ./scripts/del-types.ts",
		"typedoc": "typedoc --options typedoc.config.mjs --tsconfig tsconfig.typedoc.json",
		"do-docs-dev": "vitepress dev src --port 8080",
		"do-docs-build": "vitepress build src"
	},
	"dependencies": {
		"@antfu/utils": "^9.2.0",
		"@element-plus/icons-vue": "^2.3.1",
		"@form-create/designer": "^3.2.6",
		"@form-create/element-ui": "^3.2.8",
		"@logicflow/core": "^2.0.5",
		"@logicflow/extension": "^2.0.9",
		"@vueuse/core": "^12.7.0",
		"@vueuse/integrations": "^12.7.0",
		"@wangeditor/editor": "^5.1.23",
		"@wangeditor/editor-for-vue": "^5.1.12",
		"axios": "^1.8.1",
		"crypto-js": "^4.2.0",
		"echarts": "^5.6.0",
		"element-plus": "^2.9.4",
		"git-cz": "^4.9.0",
		"lodash-es": "^4.17.21",
		"pdfobject": "^2.3.1",
		"pinia": "^3.0.1",
		"qs": "^6.14.0",
		"vue": "^3.5.13",
		"vue-i18n": "11.0.0-rc.1",
		"vue-plugin-hiprint": "0.0.58-fix",
		"vue-router": "^4.5.0",
		"xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.11.0",
		"@changesets/cli": "^2.28.1",
		"@ruan-cat/commitlint-config": "^1.0.4",
		"@ruan-cat/domains": "^0.4.0",
		"@ruan-cat/utils": "^4.2.1",
		"@ruan-cat/vercel-deploy-tool": "^0.8.10",
		"@ruan-cat/vitepress-preset-config": "^0.9.0",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/lodash-es": "^4.17.12",
		"@types/qs": "^6.9.18",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vitest/ui": "^3.0.6",
		"@vue/eslint-config-prettier": "^10.2.0",
		"@vue/test-utils": "^2.4.6",
		"commitizen": "^4.3.1",
		"consola": "^3.4.0",
		"cpx": "^1.5.0",
		"cross-env": "^7.0.3",
		"cz-git": "^1.11.1",
		"del": "^8.0.0",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-format": "^1.0.1",
		"eslint-plugin-prettier": "^5.2.5",
		"jsdom": "^26.0.0",
		"pinia-plugin-persistedstate": "^4.2.0",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-lint-md": "^1.0.1",
		"sass-embedded": "^1.85.0",
		"tsx": "^4.19.3",
		"turbo": "^2.4.3",
		"type-plus": "^7.6.2",
		"typedoc": "0.28.0-beta.0",
		"typedoc-plugin-frontmatter": "^1.2.1",
		"typedoc-plugin-markdown": "^4.4.2",
		"typescript": "^5.7.3",
		"unplugin-auto-import": "^19.1.0",
		"unplugin-element-plus": "^0.9.1",
		"unplugin-icons": "^22.1.0",
		"unplugin-vue-components": "^28.4.0",
		"unplugin-vue-router": "^0.10.9",
		"vite": "^6.2.1",
		"vite-plugin-autogeneration-import-file": "^3.0.0",
		"vite-plugin-html": "^3.2.2",
		"vite-plugin-ts-alias": "^0.1.1",
		"vite-plugin-vue-devtools": "^7.7.2",
		"vite-plugin-vue-meta-layouts": "^0.5.1",
		"vitepress": "^1.6.3",
		"vitepress-demo-plugin": "^1.3.1",
		"vitest": "^3.0.6"
	}
}
